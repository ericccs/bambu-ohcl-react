{
    "extends": [
        "tslint:recommended", "tslint-react", "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [false, "log", "error", "warning"],
        "ordered-imports": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts",
            "node_modules/**/*.tsx",
            "src/**/*.css"
        ]
    }
}